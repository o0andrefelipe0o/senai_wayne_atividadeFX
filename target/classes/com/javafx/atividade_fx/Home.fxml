<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="589.0" prefWidth="841.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javafx.atividade_fx.Controller.HomeController">
    <children>
        <Pane prefHeight="640.0" prefWidth="903.0">
            <children>
                <ImageView fitHeight="421.0" fitWidth="520.0" layoutX="183.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@logo/logo.png" />
                    </image>
                </ImageView>

                <Button layoutX="289.0" layoutY="423.0" mnemonicParsing="false" onAction="#abrirLogin" prefHeight="53.0" prefWidth="208.0" text="Login">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
                <ButtonBar layoutX="183.0" layoutY="32.0" prefHeight="40.0" prefWidth="200.0" />
                <Button layoutX="31.0" layoutY="32.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="95.0" text="Sobre" textOverrun="WORD_ELLIPSIS" onAction="#mostrarSobre">
                <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                    <effect>
                        <Blend mode="ADD" />
                    </effect>
                </Button>
            </children>
        </Pane>
    </children>
</VBox>